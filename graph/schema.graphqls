# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Session {
  id: ID!
  code: String!
}

type Player {
  name: String!
  session: Session!
}

type Query {
  gamesList: [String!]!
  sessions: [Session!]!
}

type Mutation {
  startSession(gameName: String): Session!
  joinSession(name: String!, code: String!): Player!
}
