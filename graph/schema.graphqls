# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Session {
  id: ID!
  code: String!
}

type Participant {
  name: String!
  session: Session!
}

type Query {
  sessions: [Session!]!
}

type Mutation {
  startSession: Session!
  joinSession(name: String!, code: String!): Participant!
}
